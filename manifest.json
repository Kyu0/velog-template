{
  "name": "Velog Template",
  "description": "Make post template, and use it eaily.",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": ["storage"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["injected.js"],
      "matches": ["https://velog.io/*"]
    }
  ],
  "content_scripts": [
    { "js": ["content.js"], "matches": ["https://velog.io/write*"] }
  ]
}
